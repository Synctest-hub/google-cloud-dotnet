<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <Version>1.0.0-alpha01</Version>
        <TargetFramework>netstandard2.1</TargetFramework>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <Description>Recommended Google client library to use Google Cloud Secret Manager to store data protection keys for ASP.NET Core.</Description>
        <PackageTags>Secret Manager;Google;Cloud;Security</PackageTags>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Google.Cloud.SecretManager.V1" Version="[2.1.0, 3.0.0)" />
        <PackageReference Include="Microsoft.AspNetCore.DataProtection" Version="[6.0.0, 7.0.0)" />
    </ItemGroup>
</Project>
